{
  "name": "ecocrock",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "build-android": "npx expo export --platform android && expo run:android && cd android && ./gradlew assembleRelease && cd .. && mkdir -p Releases/ && cp android/app/build/outputs/apk/release/app-release.apk Releases/app-release.apk"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@hookform/resolvers": "^4.1.3",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-ml-kit/text-recognition": "^1.5.2",
    "@react-navigation/native": "^7.0.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.7",
    "expo": "^52.0.0",
    "expo-asset": "~11.0.4",
    "expo-camera": "~16.0.18",
    "expo-constants": "^17.0.8",
    "expo-file-system": "~18.0.11",
    "expo-font": "~13.0.4",
    "expo-image": "~2.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-location": "~18.0.8",
    "expo-media-library": "~17.0.6",
    "expo-modules-core": "~2.2.3",
    "expo-router": "~4.0.18",
    "expo-splash-screen": "~0.29.22",
    "expo-sqlite": "~15.1.2",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "~4.0.8",
    "expo-web-browser": "~14.0.2",
    "nativewind": "^4.1.23",
    "node-web-worker": "^1.0.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-native": "0.76.7",
    "react-native-fieldset": "^1.0.3",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "tesseract.js": "^6.0.0",
    "tesseract.js-core": "^6.0.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~18.3.12",
    "@types/react-native-dotenv": "^0.2.2",
    "eslint": "^9.18.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "jest": "~29.7.0",
    "jest-expo": "^52.0.0",
    "prettier": "3.4.2",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.21.0"
  },
  "private": true,
  "expo": {
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  },
  "assetBundlePatterns": [
    "**/*"
  ]
}
